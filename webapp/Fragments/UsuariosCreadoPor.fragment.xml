<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog 
	id				= "Usuarios123"
	title			= "Nombres" 
	class			= "sapUiPopupWithPadding" 
	items			= "{sistema>/dataNombreIns}"
	search			= "onSearch"
	liveChange		= "onSearch"
	confirm			= "onValueHelpDialogClose" 
	showClearButton	= "false">
		
		<StandardListItem 
		iconDensityAware	="false" 
		iconInset			="false" 
		class				="listSmall2" 
		title				="{sistema>NombreIns}"/>
		
	</SelectDialog>
</core:FragmentDefinition>